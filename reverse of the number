import java.util.Scanner;
public class Reverse{
 public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter the number: ");
    int num=sc.nextInt();
    int temp=0;
    while (num != 0) {
      int a=num%10;
      temp=temp*10+a;
      num /=10;  
    }
    System.out.println("Reverse of the number"+temp);
}}
